{
  "name": "tiger-games-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start-client": "vite",
    "build": "tsc && vite build && (cd ../server && yarn)",
    "check:ts": "tsc --incremental --noEmit --project tsconfig.json",
    "check:prettier": "prettier -u --check .",
    "check:prettier:fix": "prettier -u --write .",
    "check:lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "check:lint:fix": "eslint ./src --fix",
    "check:all": "conc --kill-others-on-fail 'yarn:check:ts' 'yarn:check:prettier' 'yarn:check:lint'",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress:open": "cypress open",
    "commitlint": "commitlint --edit",
    "prepare": "cd .. && husky install client/.husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "./client yarn check:all"
      ]
    }
  },
  "dependencies": {
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.0",
    "redux": "^4.2.1",
    "sort-by": "^1.2.0",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-redux": "^7.1.31",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "commitlint": "^18.4.3",
    "concurrently": "^8.2.2",
    "cypress": "p^13.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "inquirer-directory": "^2.2.0",
    "onchange": "^7.1.0",
    "plop": "^4.0.0",
    "prettier": "3.1.0",
    "storybook": "^7.5.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
